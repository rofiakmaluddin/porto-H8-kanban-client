        method: 'POST',\n          url: this.baseUrl + 'register',\n          data: {\n            firstName: this.firstName,\n            lastName: this.lastName,\n            email: this.email,\n            password: this.password\n          }\n        })\n        .then(res => {\n          console.log(res);\n          this.changeShowPage('login')\n        })\n        .catch(err => {\n          console.log(err);\n        })\n        .then(_ => {\n          this.firstName = ''\n          this.lastName = ''\n          this.email = ''\n          this.password = ''\n        })\n      },\n  }\n}\n</script>\n\n<style>\n\n</style>"},"lineCount":null}},"error":null,"hash":"d4eb3a9d882a423da03427aa60ac4cde","cacheData":{"env":{}}}